{
  "name": "@rentalshop/api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3002",
    "dev:prod": "NEXT_PUBLIC_API_URL=https://api.anyrent.shop NODE_ENV=production next dev -p 3002",
    "dev:dev": "NEXT_PUBLIC_API_URL=https://dev-api.anyrent.shop NODE_ENV=development next dev -p 3002",
    "dev:local": "NEXT_PUBLIC_API_URL=http://localhost:3002 NODE_ENV=development next dev -p 3002",
    "dev:all:development": "NEXT_PUBLIC_API_URL=https://dev-api.anyrent.shop NODE_ENV=development next dev -p 3002",
    "dev:all:production": "NEXT_PUBLIC_API_URL=https://api.anyrent.shop NODE_ENV=production next dev -p 3002",
    "build": "npx prisma generate --schema=../../prisma/schema.prisma && SKIP_ENV_VALIDATION=true next build",
    "build:railway": "npx prisma migrate deploy --schema=../../prisma/schema.prisma && npx prisma generate --schema=../../prisma/schema.prisma && next build",
    "start": "next start -p 3002",
    "lint": "next lint",
    "migrate:deploy": "npx prisma migrate deploy --schema=../../prisma/schema.prisma",
    "migrate:generate": "npx prisma generate --schema=../../prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "6.16.2",
    "@rentalshop/auth": "file:../../packages/auth",
    "@rentalshop/database": "file:../../packages/database",
    "@rentalshop/errors": "file:../../packages/errors",
    "@rentalshop/middleware": "file:../../packages/middleware",
    "@rentalshop/utils": "file:../../packages/utils",
    "@rentalshop/validation": "file:../../packages/validation",
    "@types/jsonwebtoken": "^9.0.10",
    "jsonwebtoken": "^9.0.2",
    "next": "^14.0.0",
    "prisma": "6.16.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sharp": "^0.34.5",
    "swagger-ui-react": "^5.29.5",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "typescript": "^5.0.0"
  }
}
