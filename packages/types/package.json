{
  "name": "@rentalshop/types",
  "version": "1.0.0",
  "description": "Centralized type definitions for Rental Shop application",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@rentalshop/constants": "file:../constants"
  },
  "devDependencies": {
    "tsup": "^7.3.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "zod": "^3.22.0"
  }
}
